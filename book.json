        },
        "valine": {
            "appId": "CxEspSv5Qi04WQvJ6VtQEbao-gzGzoHsz",
            "appKey": "1t0g3FX6K94RxyS7qL4saF30"
        },
        "siteVerification": {
            "baidu": "code-GSvFEQ9wW6",
            "google": "rOkdwqRRumtN4B-wfhQFuDjQ4PlOjPSly1DBoK-x29w"
        },
        "sitemap-general": {
            "prefix": "https://csnote.xnzone.tk/"
