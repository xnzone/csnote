{
    "author": "xnzone",
    "title": "csnote",
    "description": "csnote",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "styles":{
        "website":"./asset/website.css"
    },
    "plugins": [
        "-highlight",
        "-search",
        "-lunr",
        "-sharing",
        "search-pro",
        "hide-published-with",
        "toggle-chapters",
        "expandable-chapters-small",
        "drainage@git+https://github.com/xnzone/gitbook-plugin-drainage.git#0.0.1",
        "prism",
        "busuanzi@git+https://github.com/xnzone/gitbook-plugin-busuanzi.git#0.0.1",
        "auto-scroll-table",
        "lightbox",
        "uri-favicon@git+https://github.com/xnzone/gitbook-plugin-uri-favicon.git#0.0.1",
        "tbfed-pagefooter",
        "back-to-top-button",
        "splitter",
        "edit-link"
    ],
    "pluginsConfig": {
        "drainage": {
            "height": "274",
            "width": "360"
        },
        "prism": {
            "css": [
              "prismjs/themes/prism-okaidia.css"
            ]
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright © xnzone 2021",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "edit-link": {
            "base": "//github.com/xnzone/csnote/edit/master",
            "label": "编辑"
       }
    }
}