{
    "author": "xnzone",
    "title": "csnote",
    "description": "csnote",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "plugins": [
        "-sharing",
        "-highlight",
        "hide-published-with",
        "toggle-chapters",
        "expandable-chapters-small",
        "drainage@git+https://github.com/xnzone/gitbook-plugin-drainage.git#0.0.1",
        "prism",
        "pageview-count",
        "auto-scroll-table",
        "lightbox",
        "uri-favicon@git+https://github.com/xnzone/gitbook-plugin-uri-favicon.git#0.0.1"
    ],
    "pluginsConfig": {
        "drainage": {
            "height": "274",
            "width": "360"
        },
        "prism": {
            "css": [
              "prismjs/themes/prism-okaidia.css"
            ]
        }
    }
}