{
    "author": "xnzone",
    "title": "csnote",
    "description": "csnote",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "plugins": [
        "-highlight",
        "-search",
        "-lunr",
        "-sharing",
        "search-pro",
        "hide-published-with",
        "chapter-fold",
        "expandable-chapters-small",
        "drainage@git+https://github.com/xnzone/gitbook-plugin-drainage.git#0.0.1",
        "prism",
        "busuanzi@git+https://github.com/xnzone/gitbook-plugin-busuanzi.git#0.0.1",
        "auto-scroll-table@git+https://github.com/xnzone/gitbook-plugin-auto-scroll-table.git#0.0.1",
        "lightbox@git+https://github.com/xnzone/gitbook-plugin-lightbox.git#0.0.1",
        "uri-favicon@git+https://github.com/xnzone/gitbook-plugin-uri-favicon.git#0.0.1",
        "tbfed-pagefooter",
        "back-to-top-button",
        "splitter",
        "edit-link",
        "katex",
        "prism-themes",
        "theme-vue@git+https://github.com/xnzone/gitbook-plugin-theme-vue.git#0.0.1",
        "valine",
        "push-bd",
        "siteverification",
        "sitemap-general"
