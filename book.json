{
    "author": "xnzone",
    "title": "csnote",
    "description": "csnote",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "plugins": [
        "-highlight",
        "-search",
        "-lunr",
        "search-pro",
        "hide-published-with",
        "toggle-chapters",
        "expandable-chapters-small",
        "drainage@git+https://github.com/xnzone/gitbook-plugin-drainage.git#0.0.1",
        "favicon",
        "prism",
        "pageview-count",
        "auto-scroll-table",
        "lightbox",
        "tbfed-pagefooter"
    ],
    "pluginsConfig": {
        "drainage": {
            "height": "274",
            "width": "360"
        },
        "favicon": {
            "shortcut": "https://gitee.com/ixnzone/img-bed/raw/master/favicon.ico",
            "bookmark": "https://gitee.com/ixnzone/img-bed/raw/master/favicon.ico",
            "appleTouch": "https://gitee.com/ixnzone/img-bed/raw/master/favicon.png",
            "appleTouchMore": {
                "120x120": "https://gitee.com/ixnzone/img-bed/raw/master/favicon.png",
                "180x180": "https://gitee.com/ixnzone/img-bed/raw/master/favicon.png"
            }
        },
        "prism": {
            "css": [
              "prismjs/themes/prism-okaidia.css"
            ]
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright © xnzone 2021",
            "modify_lable": "修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}